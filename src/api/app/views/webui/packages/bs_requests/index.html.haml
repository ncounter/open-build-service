- @pagetitle = "Requests for #{@package}"

.row
  .px-0.px-md-3.mb-3
    .card
      = render(partial: 'webui/package/tabs', locals: { project: @project, package: @package })

      .card-body
        = render(partial: 'webui/shared/bs_requests/form',
                locals: { selected_filter: @selected_filter, bs_requests_creators: @bs_requests_creators, url: @url,
                          bs_requests: @bs_requests, viewed_object: 'Package' })
